
{% assign isQuickLinks = false %}
  {% for block in section.blocks %}
    {% case block.type %}
     {% when 'menu_label' %}
      {% if block.settings.link != blank and block.settings.title != blank  %}
        {% assign isQuickLinks = true %}
      {% endif %}
      {% endcase %}
    {% endfor %}
<style>
    header{
      --logoWidth:{{ section.settings.logo_width }}px;
      --searchBorderColor:{{ settings.search_border_color }};
      --menuFontFamily:var(--{{ section.settings.menu_font }}Font);
      --submenuFontFamily:var(--{{ section.settings.sub_menu_font }}Font);
      --menuPosition:{{ section.settings.menu_items_alignment }};
      --headerOuterBackground: {{ section.settings.top_corner_color }};
        {% if section.settings.menu_items_alignment == 'center' %}
          --quickLinkPosition: center;
        {% elsif section.settings.menu_items_alignment == 'flex-end' %}
          --quickLinkPosition: flex-end;
        {% else %}
          --quickLinkPosition: space-between;
        {% endif %}
      --store-detail-dropdown-color: {{ section.settings.store_details_text_color }};
      --store-detail-dropdown-link: {{ section.settings.store_details_link_color }};
      --store-detail-dropdown-border: {{ section.settings.store_details_border_color }};
      --store-detail-dropdown-bg: {{ section.settings.store_details_dropdown_bg }};

      {% assign storeDetailDropdownBg = section.settings.store_details_dropdown_bg | color_brightness %}
      {%- if storeDetailDropdownBg < 130 -%}
        --store-detail-dropdown-hover:{{ section.settings.store_details_dropdown_bg | color_lighten: 10 }};
        {%- else -%}
        --store-detail-dropdown-hover:{{ section.settings.store_details_dropdown_bg | color_darken: 10 }};
      {%- endif -%}

      --support-content-bg: {{ section.settings.support_content_bg }};
      --support-text-color: {{ section.settings.support_content_text }};
      
      
    }
    {% if isQuickLinks and section.settings.menu_items_alignment == 'center' %}
      .secondary-header .quick-links-enable .nav-menu-list {
          width: max-content;
      }
    {% endif %}
    .inner-side .side-menu-wrapper .nav-item .nav-item-header a,
    .inner-side .mobile-category-menu-bar .nav-item .nav-item-header a,
    header .categories-submenu-link{
      font-family:var(--menuFontFamily);
    }
    .inner-side .side-menu-wrapper .nav-item .inner-child .nav-item-header a,
    .inner-side .side-menu-wrapper .nav-item .inner-child .megamenu-promotion-title,
    .inner-side .mobile-category-menu-bar .nav-item .inner-child .nav-item-header a,
    .inner-side .mobile-category-menu-bar .nav-item .inner-child .categories-inner-submenu-img-content *,
    header .categories-inner-submenu-item a,
    header .categories-inner-submenu-heading{
      font-family:var(--submenuFontFamily);
    }
    header .search-content[data-search-drawer] {
      --searchTextColor: rgb(var(--textColor));
      --headerIconColor: rgb(var(--textColor));
      --headerBg: rgb(var(--alternateBackground));
    }
    html[color-mode="dark"]   header .search-content[data-search-drawer] {
      --searchTextColor: #d1d1d1;
      --headerIconColor: #d1d1d1;
      --headerBg:#000;
    }
    {% if section.settings.enable_header_shape %}
    #shopify-section-{{ section.id }} .shapes-background {
      background: transparent;
      position: absolute;
      width: 100%;
      top: 100%;
    }
    #shopify-section-{{ section.id }}  .shapes-background svg {
      width: 100%;
      display: block;
      object-fit: var(--image-fit);
      {% unless section.settings.select_shape == 'shape8' %}
      transform: rotate(180deg);    
      {% endunless %}
      margin-top: -1px;
    }
    #shopify-section-{{ section.id }}  .shapes-background svg path,
    #shopify-section-{{ section.id }}  .shapes-background svg rect{
      {% unless section.settings.svg_color == 'rgba(0,0,0,0)' or section.settings.svg_color == blank %}
      fill: {{ section.settings.svg_color }};
      {% else %}
        {% if section.settings.header_style == 'logo_with_inline_menu' %}
          fill: {{ settings.background_header }};
        {% else %}
          fill: {{ settings.secondary_background_header }};
        {% endif %}
      {% endunless %}
    }
    #shopify-section-{{ section.id }} .shapes-background svg path.shape-stoke-path{
      {% unless section.settings.svg_stroke == 'rgba(0,0,0,0)' or section.settings.svg_stroke == blank %}
      stroke: {{ section.settings.svg_stroke }};
      {% else %}
        {% if section.settings.header_style == 'logo_with_inline_menu' %}
          stroke: {{ settings.background_header }};
        {% else %}
        stroke: {{ settings.secondary_background_header }};
        {% endif %}
      {% endunless %}
    }
    [color-mode="dark"] #shopify-section-{{ section.id }}  .shapes-background svg path,
    [color-mode="dark"] #shopify-section-{{ section.id }}  .shapes-background svg rect{
      {% if section.settings.header_style == 'logo_with_inline_menu' %}
        fill: #000;
      {% else %}
        fill: #0e0e0e;
      {% endif %}
    }
    [color-mode="dark"] #shopify-section-{{ section.id }} .shapes-background svg path.shape-stoke-path{
      {% if section.settings.header_style == 'logo_with_inline_menu' %}
        stroke:#000;
      {% else %}
      stroke: #0e0e0e;
      {% endif %}
    }
 
  {% endif %}
</style>
<link rel="preload" as="style" href="{{ 'country-flags.css' | asset_url }}">
{{ 'country-flags.css' | asset_url | stylesheet_tag }}
<div
  class="main-header{% if section.settings.enable_sticky_header%} sticky-header{% endif %}{% if section.settings.header_style == 'logo_with_inline_menu' %} logo-with-inline-menu{% endif %}{% if template == 'index' and section.settings.transparent_header %} transparent-header-true{% endif %}{% if section.settings.header_shadow %} header-shadow{% endif %}{% if section.settings.show_top_corner_radius %} radius-enabled{% endif %}"
  {% if section.settings.enable_sticky_header %}
    data-sticky-type="{{ section.settings.sticky_header_type}}"
  {% endif %}
>
  <div
    class="primary-header"
    data-header-section
  >
    <div class="container">
      <div class="primary-header-blocks logo-{{ section.settings.logo_alignment }}">
        {% liquid
          assign badgeBlocks = section.blocks | where: 'type', 'promo_badge'
          capture promoBadges
            for block in badgeBlocks
              render 'header-promo-badges', block: block
            endfor
          endcapture
          render 'mobile-menu',section:section
          assign headerNavigationStatus = false
          if section.settings.menu != blank or promoBadges != blank
            assign headerNavigationStatus = true
          elsif section.settings.category_heading != blank and section.settings.category_menu != blank
            assign headerNavigationStatus = true
          endif
          if section.settings.header_style == 'logo_with_humburger_menu'
          render 'hamburger-menu', section: section
         endif
          if section.settings.logo_alignment == 'left'
            render 'header-logo', section: section
          endif
          if section.settings.header_style == 'logo_with_search_bar'
            if section.settings.enable_search
              render 'header-search-bar', barStatus: true
            else
              echo '<div class="d-none d-md-block"></div>'
            endif
          endif
          if section.settings.header_style == 'logo_with_inline_menu' and headerNavigationStatus
            render 'header-navigation', section: section, badgesData: promoBadges
          endif
        
          if section.settings.logo_alignment == 'center'
            render 'header-logo', section: section
          endif
          echo '<div class="header-options">'

          if section.settings.store_content != blank and section.settings.store_detail != blank and section.settings.enable_store_details
          render 'support-details', section: section
          endif

          if settings.enable_dl_mode_toggle
            render 'color-mode', section: section, device:'desktop' 
          endif
          assign headerIcons = false
          if section.settings.enable_cart
            assign headerIcons = true
          endif
          if section.settings.enable_account and shop.customer_accounts_enabled
            assign headerIcons = true
          endif
          if section.settings.header_style == 'logo_with_inline_menu' and section.settings.enable_search
            assign headerIcons = true
          endif
          if headerIcons
            render 'header-icons', section: section
          endif
          echo '</div>'
        %}
      </div>
    </div>
  </div>
  

  {% if section.settings.header_style == 'logo_with_search_bar' and headerNavigationStatus %}
    <div class="secondary-header d-none d-lg-block">
      <div class="container">
        {% render 'header-navigation', section: section, badgesData: promoBadges %}
      </div>
    </div>
  {% endif %}
   
      {% render 'predictive-search-drawer', fullWidth: true %}


  {% if section.settings.enable_header_shape %}
    <div class="shapes-background">
      {% render 'shapes', shape: section.settings.select_shape %}
    </div>
  {% endif %}
</div>

