{{ 'multiboxes.css' | asset_url | stylesheet_tag }}
{{ 'slideshow.css' | asset_url | stylesheet_tag }}

{% liquid
  assign Colorscheme = section.settings.color_scheme
%}
{% if section.blocks.size > 0 %}
  <div class="scheme-{{ Colorscheme}} section-padding{% if section.settings.enable_gradient %} gradiant{% endif %}">
  <div class="{% if section.settings.enable_fullwidth %}multibox-fullwidth{% else %}container{% endif %}">
      <div class="categories-boxes">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'text-with-image' %}
              {% render 'multiboxes-text-icons',
                section: section,
                block: block,
                loopIndex: forloop.index,
                blockColumn: block.settings.column
              %}
            {% when 'slideshow' %}
              {% render 'multiboxes-slideshow', section: section, block: block, loopIndex: forloop.index %}
            {% when 'media-grid' %}
              {% render 'multiboxes-media-grid', section: section, block: block, loopIndex: forloop.index %}
            {% when 'filters' %}
              {% render 'quick-nav', section: section, block: block, loopIndex: forloop.index %}
          {% endcase %}
        {% endfor %}
      </div>
    </div>
  </div>
{% endif %}
<style>
    #shopify-section-{{ section.id }}{
      --padding-top: {{ section.settings.top_space_desktop}}px;
      --padding-bottom: {{ section.settings.bottom_space_desktop}}px;
      --column-gap: {{ section.settings.column_gap }}px;
    }
    {% for block in section.blocks %}
      #block-{{ block.id }}{
        --headingColor: {{ block.settings.heading_color }};
        --categoryBgColor:{{ block.settings.icon_background_color }};
        --categoryBgHoverColor:{{ block.settings.icon_background_hover }};
        --categoryTextColor: {{ block.settings.text_color }};
        --categoryTextHoverColor:{{ block.settings.text_hover_color }};
        {%- if block.settings.background_style == 'gradient' -%}
          --categoryBackground: {{ block.settings.background_gradient }};
          {%- else -%}
          --categoryBackground: {{ block.settings.background_color }};
        {%- endif -%}
        --opacityColor:{{ block.settings.background_color_overlay.red }},{{ block.settings.background_color_overlay.green }},{{ block.settings.background_color_overlay.blue }};
        --backgroundOpacity:{{ block.settings.background_opacity }};
        --slideshow-heading-color:{{block.settings.text_color }};
        --slideshow-text-color: {{ block.settings.description_color }};
        --overlay-bg:{{ block.settings.overlay_color }};
        --overlay-opacity:{{ block.settings.overlay_opacity | divided_by:100.0 }};
        --linkColor: {{ block.settings.btn_text_color.red }},{{ block.settings.btn_text_color.green }},{{ block.settings.btn_text_color.blue }};
        --buttonBackground: {{ block.settings.btn_bg_color.red }},{{ block.settings.btn_bg_color.green }},{{ block.settings.btn_bg_color.blue }};
        {% unless block.settings.btn_shadow_color == 'rgba(0,0,0,0)' or block.settings.btn_shadow_color == blank %}
          --buttonShadow:{{ block.settings.btn_shadow_color }};
        {% endunless %}
        --buttonColor: {{ block.settings.btn_text_color.red }},{{ block.settings.btn_text_color.green }},{{ block.settings.btn_text_color.blue }};
        --buttonBorder: {{ block.settings.btn_border_color.red }},{{ block.settings.btn_border_color.green }},{{ block.settings.btn_border_color.blue }};
        --buttonBackgroundHover:{{ block.settings.btn_hover_color.red }},{{ block.settings.btn_hover_color.green }},{{ block.settings.btn_hover_color.blue }};
        --buttonColorHover:{{ block.settings.btn_hover_text_color.red }},{{ block.settings.btn_hover_text_color.green }},{{ block.settings.btn_hover_text_color.blue }};
        --buttonBorderHover:{{ block.settings.btn_hover_border_color.red }},{{ block.settings.btn_hover_border_color.green }},{{ block.settings.btn_hover_border_color.blue }};
        --textHighlightColor: {{ block.settings.text_highlight_color }};
        --cardBackground:{{ block.settings.card_background }};
        --GridtextColor:{{ block.settings.text_color }};
        --GridheadingColor: {{ block.settings.heading_color }};
        --GridheadinghighlightColor: {{ block.settings.text_highlight_color }};
        --GridopacityColor:{{ block.settings.background_color.red }},{{ block.settings.background_color.green }},{{ block.settings.background_color.blue }};
        --GridbuttonBackground:{{ block.settings.btn_bg_color.red }},{{ block.settings.btn_bg_color.green }},{{ block.settings.btn_bg_color.blue }};
        --GridbuttonBorder:{{ block.settings.btn_border_color.red }},{{ block.settings.btn_border_color.green }},{{ block.settings.btn_border_color.blue }};
        --GridbuttonColor:{{ block.settings.btn_text_color.red }},{{ block.settings.btn_text_color.green }},{{ block.settings.btn_text_color.blue }};
        --GridbuttonBackgroundHover:{{ block.settings.btn_hover_color.red }},{{ block.settings.btn_hover_color.green }},{{ block.settings.btn_hover_color.blue }};
        --GridbuttonColorHover:{{ block.settings.btn_hover_text_color.red }},{{ block.settings.btn_hover_text_color.green }},{{ block.settings.btn_hover_text_color.blue }};
        --GridbuttonBorderHover:{{ block.settings.btn_hover_border_color.red }},{{ block.settings.btn_hover_border_color.green }},{{ block.settings.btn_hover_border_color.blue }};
        --column:{{ block.settings.column }};
      }
      [color-mode="dark"] #block-{{ block.id }}{
        --categoryBgColor: #161616;
        --categoryTextColor: #d1d1d1;
        --categoryBgHoverColor:#ffca10;
      }
    {% endfor %}
    @media (min-width: 768px){
      #shopify-section-{{ section.id }}{
      --desktopHeight: {{ section.settings.desktop_row_height}}px;
      }
    }
    @media only screen and (max-width:767px) {
      #shopify-section-{{ section.id }}{
        --padding-top: {{ section.settings.top_space_mobile }}px !important;
        --padding-bottom: {{ section.settings.bottom_space_mobile }}px !important;
        --mobileHeight:{{ section.settings.mobile_row_height}}px;
      }
    }
    {% if section.settings.section_visibility == 'large' %}
      @media (max-width: 767px) {
        #shopify-section-{{ section.id }}{
          display: none !important;
        }
      }
    {% elsif section.settings.section_visibility == 'small' %}
      @media (min-width: 768px) {
        #shopify-section-{{ section.id }}{
          display: none !important;
        }
      }
  {% endif %}
</style>
